{
  "manifest_version": 2,
  "name": "Teachable Keyboard Shortcut",
  "version": "1.0",
  "description": "test",
  "permissions": [
    "activeTab",
    "https://www.example.com/*"
  ],
  "background": {
    "scripts": ["background.js"]
  },
  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "popup.html"
  }
}
